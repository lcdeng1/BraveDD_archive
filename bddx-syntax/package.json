{
    "name": "bddx-syntax",
    "displayName": "BDDX Syntax Highlighting",
    "description": "Syntax highlighting for BDDX files",
    "version": "0.1.6",
    "engines": {
      "vscode": "^1.50.0"
    },
    "contributes": {
      "languages": [
        {
          "id": "bddx",
          "aliases": ["BDDX", "bddx"],
          "extensions": [".bddx"]
        }
      ],
      "grammars": [
        {
          "language": "bddx",
          "scopeName": "source.bddx",
          "path": "./syntaxes/bddx.tmLanguage.json"
        }
      ]
    }
  }
  